{
  "manifest_version": 3,
  "name": "WebGation",
  "short_name": "WebGation",
  "version": "5.57",
  "version_name": "v5.57_build_57",
  "author": "MicroResearch CorporationÂ®",
  "description": "WebGation is a lightweight, high-performance Chrome Extension designed to enhance your browsing speed.",
  "minimum_chrome_version": "130",
  "offline_enabled": false,
  "homepage_url": "https://microresearch-corporation.github.io/",
  "icons": {
    "16": "./src/16.png",
    "48": "./src/48.png",
    "128": "./src/128.png",
    "256": "./src//logo.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "WebGation",
    "browser_style": true,
    "default_icon": {
      "16": "./src/16.png",
      "48": "./src/48.png",
      "128": "./src/128.png",
      "256": "./src//logo.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "incognito": "split",
  "web_accessible_resources": [
    {
      "resources": ["static.js", "floating.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
